{
  "name": "lipy-dexdump",
  "group": "com.linkedin.@spec.name@",
  "version": "0.0.*",
  "scmUrl": "ssh://git.corp.linkedin.com:29418/multiproducts/lipy-dexdump.git",
  "owners": [
    "ayamanak",
    "jrusnak",
    "stopiwal"
  ],
  "comment": "",
  "description": "Tool for parsing dex/apk files for test info",
  "watchers": [],
  "build": {
    "commands": {
      "build": "ligradle -Prelease=true -PallArtifacts build",
      "precommit": "ligradle flake8",
      "snapshot": "ligradle -Psnapshot=true build",
      "test": "echo 'testing done by build command'"
    },
    "toolchains": {
      "gradle": {
        "version": "3.5",
        "plugins": {
          "gradle-python": {
            "libraries": [
              "gradle-python"
            ],
            "version": "1.*"
          },
          "ligradle-jvm": {
            "libraries": [
              "product-plugins"
            ],
            "version": "2.*.*"
          }
        }
      }
    },
    "type": "gradle",
    "versions": {
      "java": "1.8.0_121"
    }
  },
  "runtime": {},
  "review": {
    "wcTest": true
  },
  "codecoveragecheckinprecommit": {},
  "topology": {
    "applications": {}
  },
  "archive": {
    "artifacts": {},
    "databases": {},
    "repo_name": "pypi-internal"
  },
  "dependencyResolution": {},
  "external": {
    "six": "pypi:six:1.10.0"
  },
  "product": {},
  "trunkDev": {
    "autoRevert": true
  },
  "extSCM": {}
}

